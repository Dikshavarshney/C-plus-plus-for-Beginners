#include<iostream>
using namespace std;

int main(){

    float num1,num2;
    float add,sub,multi,div;

    cout<<"\nEnter number1: ";
    cin>>num1;

    cout<<"\nEnter number2 :";
    cin>>num2;
     
    cout<<"\nThe addition of num1+num2 = "<<num1+num2;    
    cout<<"\nThe of substraction num1-num2 = "<<num1-num2;    
    cout<<"\nThe of multiplication num1*num2 = "<<num1*num2;  
    cout<<"\nThe of division num1/num2 = "<<num1/num2;
    
    return 0;
}
